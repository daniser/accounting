{
    "name": "daniser/accounting",
    "description": "Accounting library",
    "type": "library",
    "keywords": ["accounting", "ledger"],
    "license": "MIT",
    "support": {
        "source": "https://github.com/daniser/accounting"
    },
    "authors": [
        {
            "name": "Sergey Danilchenko",
            "email": "daniser@mail.ru"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-mbstring": "*",
        "illuminate/contracts": ">=5.8",
        "illuminate/support": ">=5.8",
        "illuminate/database": ">=5.8",
        "moneyphp/money": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "Daniser\\Accounting\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Daniser\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Daniser\\Accounting\\AccountingServiceProvider"
            ]
        }
    },
    "suggest": {
        "illuminate/events": "Required to utilize transaction events (>=5.8)."
    }
}
